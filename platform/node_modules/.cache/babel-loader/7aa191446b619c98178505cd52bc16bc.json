,IAbA,kBAaA;AAAA;;AACA,MAAA,OAAA,CAAA;AACA,QAAA,QAAA,EAAA,KAAA,MAAA,CAAA,EADA;AAEA,QAAA,QAAA,EAAA,KAAA,QAFA;AAGA,QAAA,OAAA,EAAA,KAAA;AAHA,OAAA,CAAA,CAKA,IALA,CAKA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,GAAA;;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,IAAA;AACA,OARA,EASA,KATA,CASA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,KAAA,CAAA,GAAA,EAAA,CAAA;AACA,OAXA;AAYA;AA1BA;AAnBA,CAAA","sourcesContent":["<template>\n  <div class=\"one\">\n    <h3 class=\"top\">课程讨论</h3>\n    <el-divider></el-divider>\n    <el-card class=\"box-card\">\n      <div style=\"margin: 20px 0;\"></div>\n      <el-input\n        type=\"textarea\"\n        placeholder=\"请输入内容\"\n        v-model=\"textarea\"\n        maxlength=\"30\"\n        show-word-limit\n      ></el-input>\n\n      <el-button type=\"primary\" @click=\"GKD2\">提交</el-button>\n      <el-divider></el-divider>\n\n      <ul class=\"mcd-menu\" style=\"margin: 5px\">\n        <li v-for=\"item in items\" :key=\"item.id\">\n            <i class></i>\n            <strong>来自{{item.fromUserId}}的评论:</strong>\n            <small>{{item.comment}}</small>\n        </li>\n      </ul>\n    </el-card>\n  </div>\n</template>\n\n<script>\nimport { getCookie, setCookie, removeCookie } from \"@/libs/cookie\";\nimport { getCommentsById, comment } from \"@/api/modules/course\";\nexport default {\n  name: \"bbs\",\n  // 定义props属性，用于接收父组件所传递过来的数据\n  props: [\"course\"],\n  data() {\n    return {\n      //评论区内容\n      textarea: \"\",\n      items: \"\",\n      myuser: \"\",\n      username: \"\",\n      ChapterByCourseId: \"\"\n    };\n  },\n  mounted() {\n    this.myuser = JSON.parse(getCookie(\"user\") || \"{}\");\n    this.username = this.myuser.username;\n    this.GKD();\n  },\n  methods: {\n    //查询所有评论\n    GKD() {\n      getCommentsById({ courseId: this.course.id }) //名字\n        .then(res => {\n          this.items = res.data.data;\n          console.log(this.items);\n        })\n        .catch(err => {\n          this.toast(err, 2);\n        });\n    },\n    //添加评论内容\n    GKD2() {\n      comment({\n        courseId: this.course.id,\n        username: this.username,\n        comment: this.textarea\n      })\n        .then(res => {\n          this.GKD();\n          this.textarea = null;\n        })\n        .catch(err => {\n          this.toast(err, 2);\n        });\n    }\n  }\n};\n</script>\n\n<style scoped>\n.top {\n  margin-right: 1100px;\n}\n.one {\n  border-style: solid;\n  border-width: 0.1px;\n  border-radius: 10px;\n  border-color: #edeeef;\n  background-color: white;\n  margin-left: 0px;\n}\n.box-card {\n  width: 800px;\n  margin: 0 auto;\n}\n</style>"],"sourceRoot":"src/components/learn"}]}